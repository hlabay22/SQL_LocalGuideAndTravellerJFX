            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="btnOK" layoutX="180.0" layoutY="199.0" mnemonicParsing="false" onAction="#btnOKclick" prefHeight="36.0" prefWidth="103.0" text="OK">
