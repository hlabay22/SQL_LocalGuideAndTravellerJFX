<AnchorPane prefHeight="282.0" prefWidth="465.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ContactLocalGuideController">
